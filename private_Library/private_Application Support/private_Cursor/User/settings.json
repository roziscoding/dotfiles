{
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always",
      "source.organizeImports": "always"
    }
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "codesnap-plus.containerPadding": "0",
  "codesnap-plus.showWindowControls": false,
  "codesnap-plus.showWindowTitle": true,
  "codesnap-plus.shutterAction": "copy",
  "codesnap-plus.target": "window",
  "cursor.cpp.disabledLanguages": [
    "scminput"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 1.8,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [
    {
      "color": "#f9e1af6f",
      "column": 80
    },
    {
      "color": "#f38ba875",
      "column": 120
    }
  ],
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "eslint.format.enable": true,
  "explorer.compactFolders": true,
  "explorer.fileNesting.enabled": true,
  "explorer.sortOrder": "foldersNestsFiles",
  "files.associations": {
    ".env*": "properties"
  },
  "files.autoSave": "onFocusChange",
  "git.confirmSync": false,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.views.contributors.files.layout": "tree",
  "http.proxySupport": "off",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.external.osxExec": "kitty",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontFamily": "JetbrainsMono Nerd Font Mono",
  "terminal.integrated.scrollback": 10000000000000,
  "terminal.integrated.shellIntegration.decorationsEnabled": "gutter",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.showExitAlert": false,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "vim.cursorStylePerMode.replace": "underline",
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.dialogStyle": "custom",
  "window.nativeTabs": false,
  "window.titleBarStyle": "custom",
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "catppuccin-perfect-mocha",
  "workbench.preferredLightColorTheme": "Catppuccin Latte",
  "workbench.sideBar.location": "right",
  "workbench.tree.indent": 20,
  "eslint.useESLintClass": true,
  "editor.semanticHighlighting.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "never",
      "source.fixAll.ts": "never",
      "source.addMissingImports.ts": "never",
      "source.organizeImports": "never",
      "source.removeUnused.ts": "never",
      "source.removeUnusedImports": "never",
      "source.sortImports": "never"
    }
  },
  "editor.inlayHints.enabled": "offUnlessPressed",
  "vim.smartRelativeLine": true,
  "vue.server.hybridMode": true,
  "disableTsErrors.enableCustomizations": true,
  "disableTsErrors.customizations": [
    {
      "code": [
        80006
      ],
      "severity": "off"
    }
  ],
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "workbench.preferredHighContrastColorTheme": "Cursor Dark High Contrast",
  "workbench.preferredHighContrastLightColorTheme": "Catppuccin Latte",
  "window.autoDetectHighContrast": false,
  "vitest.cliArguments": "--expect.requireAssertions",
  "vitest.configSearchPatternExclude": "{**/node_modules/**,**/.*/**,**/*.d.ts}",
  "git.autofetch": true
}
